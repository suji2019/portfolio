{
  "name": "gatsby-starter-portfolio-jodie",
  "private": true,
  "description": "Image-heavy photography portfolio with colorful accents & great typography",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "cross-env CYPRESS_SUPPORT=y gatsby develop",
    "dev": "gatsby develop -o",
    "start": "yarn develop",
    "serve": "gatsby serve",
    "lint": "eslint . --ext .ts,.tsx --ignore-path .gitignore",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write content/**/*.yaml",
    "type-check": "tsc --noEmit",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:ci": "cypress run --browser chrome --reporter junit --reporter-options 'mochaFile=results/cypress/result.xml'",
    "server:open": "start-server-and-test develop http://localhost:8000 cy:open",
    "server:run": "start-server-and-test develop http://localhost:8000 cy:run",
    "server:ci": "start-server-and-test develop http://localhost:8000 cy:run:ci",
    "test:compile": "tsc --project cypress/tsconfig.json",
    "test:compile:watch": "tsc --watch --project cypress/tsconfig.json",
    "test": "npm-run-all -s test:compile server:run",
    "test:watch": "npm-run-all -p test:compile:watch server:open",
    "test:ci": "npm-run-all -s test:compile server:ci"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "acorn": "^6.4.1",
    "babel-plugin-styled-components": "^1.10.7",
    "codemirror": "^5.55.0",
    "gatsby": "^2.23.3",
    "gatsby-image": "^2.4.7",
    "gatsby-plugin-feed-mdx": "^1.0.1",
    "gatsby-plugin-google-analytics": "^2.3.4",
    "gatsby-plugin-manifest": "^2.4.11",
    "gatsby-plugin-mdx": "^1.2.14",
    "gatsby-plugin-netlify": "^2.3.5",
    "gatsby-plugin-netlify-cms": "^4.3.7",
    "gatsby-plugin-offline": "^2.0.25",
    "gatsby-plugin-react-helmet": "^3.0.12",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-sitemap": "^2.4.5",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-plugin-typescript": "^2.4.6",
    "gatsby-plugin-typography": "^2.5.8",
    "gatsby-remark-copy-linked-files": "^2.0.11",
    "gatsby-remark-images": "^3.3.9",
    "gatsby-remark-responsive-iframe": "^2.1.1",
    "gatsby-remark-smartypants": "^2.0.9",
    "gatsby-remark-vscode": "^2.1.2",
    "gatsby-source-filesystem": "^2.0.29",
    "gatsby-source-instagram": "^0.7.2",
    "gatsby-source-pg": "^0.1.4",
    "gatsby-transformer-remark": "^5.25.1",
    "gatsby-transformer-sharp": "^2.1.18",
    "gatsby-transformer-yaml": "^2.4.4",
    "netlify-cms-app": "^2.9.1",
    "netlify-cms-lib-widgets": "^1.3.0",
    "polished": "^3.6.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-refresh": "^0.8.3",
    "react-spring": "^8.0.27",
    "react-typography": "^0.16.19",
    "redux": "^2.0.0",
    "styled-components": "^5.1.1",
    "styled-system": "^5.1.5",
    "typeface-work-sans": "^0.0.72",
    "typescript": "^3.9.5",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "lodash": "^4.17.15",
    "@testing-library/cypress": "^6.0.0",
    "@types/jest": "^25.2.3",
    "@types/node": "^10.17.26",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^5.0.16",
    "@types/styled-components": "^5.1.0",
    "@types/styled-system": "^5.1.9",
    "@types/testing-library__cypress": "^5.0.5",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "cross-env": "^7.0.2",
    "cypress": "^3.8.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-cypress": "^0.4.4",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0"
  },
  "resolutions": {
    "cypress": "^4.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LekoArts/gatsby-starter-portfolio-jodie"
  },
  "bugs": {
    "url": "https://github.com/LekoArts/gatsby-starter-portfolio-jodie/issues"
  }
}
